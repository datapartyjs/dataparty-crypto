{
  "name": "@dataparty/crypto",
  "private": false,
  "version": "1.0.3",
  "description": "dataparty crypto routines",
  "main": "./dist/dataparty-crypto.js",
  "browser": "dist/dataparty-crypto-browser.js",
  "source": "./src/index.ts",
  "targets": {
    "browser": {
      "context": "browser",
      "source": "src/index.ts",
      "includeNodeModules": true,
      "engines": {
        "browsers": "Chrome 80"
      }
    },
    "main": {
      "context": "node",
      "includeNodeModules": true
    }
  },
  "dependencies": {
    "@panva/hkdf": "^1.0.4",
    "@stablelib/base64": "^0.7.2",
    "bip39": "^3.1.0",
    "buffer": "^5.2.1",
    "debug": "^4.1.1",
    "tweetnacl": "^1.0.1"
  },
  "files": [
    "dist",
    "src",
    "index.js",
    "index.d.ts",
    "webpack.config.js"
  ],
  "devDependencies": {
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.15",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "ngzip": "^1.1.3",
    "ntee": "^2.0.0",
    "parcel": "^2.7.0",
    "process": "^0.11.10",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "watch": "./node_modules/.bin/babel src --watch -d dist --extensions \".ts,.js\" --source-maps inline",
    "test": "npx jest",
    "build": "parcel build --no-scope-hoist",
    "prepare": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datapartyjs/dataparty-crypto.git"
  },
  "author": "RosHub Inc. <code@roshub.io>",
  "license": "Apache-2.0"
}
